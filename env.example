SMTP_PORT=587
SMTP_USER=
SMTP_PASSWORD=

# Rate Limiting
RATE_LIMIT_PER_MINUTE=100
RATE_LIMIT_PER_HOUR=1000

# Logging
LOG_LEVEL=INFO 